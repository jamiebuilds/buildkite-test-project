{
  "name": "buildkite-test-project",
  "version": "1.0.0",
  "description": "A test project for Buildkite",
  "main": "index.js",
  "repository": "https://github.com/jamiebuilds/buildkite-test-project",
  "author": "Jamie Kyle <me@thejameskyle.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint packages/*/src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "build": "bolt workspaces exec --parallel-nodes -- babel src -d dist --extensions \".ts,.tsx\" --config-file $(pwd)/babel.config.js --ignore '**/__tests__/**'"
  },
  "bolt": {
    "workspaces": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.1.1",
    "@types/react": "^16.4.14",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6",
    "unstated": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.1",
    "@babel/core": "^7.1.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.2",
    "ava": "^0.25.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "bolt": "^0.22.4",
    "chunkd": "^1.0.0",
    "ci-parallel-vars": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-typescript": "^1.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "is-ci": "^1.2.1",
    "jest": "^22.4.4",
    "jest-config": "^23.6.0",
    "react-test-renderer": "^16.3.2",
    "regenerator-runtime": "^0.12.1",
    "typescript": "^3.1.1"
  }
}
